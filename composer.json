{
    "name": "teh9/laravel2fa",
    "description": "A simple two-factor authentication via telegram implementation for Laravel",
    "license": "MIT",
    "keywords": [
        "telegram",
        "laravel",
        "2fa",
        "two-factor",
        "auth"
    ],
    "autoload": {
        "psr-4": {
            "Teh9\\Laravel2fa\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tengiz 'teh9' Gusev",
            "email": "tengizs.gusevs@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Teh9\\Laravel2fa\\TelegramTwoFactorServiceProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
